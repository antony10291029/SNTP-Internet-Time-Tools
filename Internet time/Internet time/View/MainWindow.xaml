<Window x:Class="InternetTime.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:viewmodel="clr-namespace:InternetTime.ViewModel"
        mc:Ignorable="d"
        Title="MainWindow" Height="429" Width="649" ResizeMode="NoResize"
        >
    <Window.DataContext>
        <viewmodel:MainWindowViewModel/>
    </Window.DataContext>
    <Grid>
        <Image Margin="-20,-20,513,270" Source="/InternetTime;component/time.png" Stretch="Fill"/>
        <ComboBox Name="ServerChoosingComboBox" HorizontalAlignment="Left" Height="27" Margin="135,10,0,0" VerticalAlignment="Top" Width="127" ItemsSource="{Binding ServerAddresses}">
        </ComboBox>

    </Grid>
</Window>
